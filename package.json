  "dependencies": {
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "@types/pg": "7.4.10",
    "ava": "1.0.0-beta.8",
    "coveralls": "3.0.2",
    "nyc": "13.0.1",
    "typescript": "3.0.3"
  },
  "scripts": {
    "prepare": "tsc -d",
    "pretest": "tsc",
