  "description": "PostgreSQL metadata aggregator",
  "keywords": [
    "metadata",
    "database",
    "postgres",
    "psql"
  ],
  "homepage": "https://github.com/chbrown/pg-meta",
  "repository": "github:chbrown/pg-meta",
